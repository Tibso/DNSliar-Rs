[Unit]
Description=dnsblrsd
After=network.target
ConditionPathExists=/home/thibaut/Desktop/rustproj/DnsBlackList-rs/dnsblrsd/dnsblrsd.conf


[Service]
User=thibaut
Group=thibaut
Restart=on-failure
RestartSec=2
StartLimitInterval=0
Type=simple
Restart=always
ExecStart=/home/thibaut/Desktop/rustproj/DnsBlackList-rs/dnsblrsd/target/release/dnsblrsd
WorkingDirectory=/home/thibaut/Desktop/rustproj/DnsBlackList-rs/dnsblrsd
StandardOutput=journal
StandardError=journal
SyslogIdentifier=dnsblrsd
Environment="TOKIO_WORKER_THREADS=8"
AmbientCapabilities=CAP_NET_BIND_SERVICE

[Install]
WantedBy=multi-user.target